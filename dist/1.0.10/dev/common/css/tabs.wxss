.tab-container {
  background-color: #fff;
}
.tab-container .menu-box {
  background: #f3f3f3;
  color: #333;
  font-size: 10px;
  height: 100rpx;
}
.tab-container .menu-box .menu-item {
  flex: 1;
  text-align: center;
  margin-bottom: 8rpx;
}
.tab-container .menu-box .menu-item.active {
  color: #4caf50;
}
.tab-container .menu-box .item-icon:before {
  font-size: 20px;
}

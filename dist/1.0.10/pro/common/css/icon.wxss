.icon-dot,
.icon-dot-small,
.icon-dot-more {
  background: #f00;
  background: #f00;
  color: #fff;
  display: block;
  font-size: 10px;
  width: 20rpx;
  height: 20rpx;
  border-radius: 20rpx;
}
.icon-dot-small {
  width: 16rpx;
  height: 16rpx;
  border-radius: 16rpx;
}
.icon-dot-more {
  border-radius: 20rpx;
  padding: 0 4px;
}

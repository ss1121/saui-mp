.actionMask {
  position: absolute;
  top: 0;
  left: 0;
  display: none;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0,0,0,0.5);
}
.actionMask.show {
  display: block;
}
.actionSide {
  position: absolute;
  right: -100vw;
  top: 0;
  transition: all 0.2s ease;
}
.actionSide.bar {
  width: 60vw;
}
.actionSide.moveit {
  transform: translate3d(-100vw, 0, 0);
}
.actionSide-left {
  position: absolute;
  left: -100vw;
  top: 0;
  transition: all 0.2s ease;
}
.actionSide-left.bar {
  width: 60vw;
}
.actionSide-left.moveit {
  transform: translate3d(100vw, 0, 0);
}
.actionSide-bot {
  position: absolute;
  left: 0;
  bottom: -100vh;
  transition: all 0.2s ease;
}
.actionSide-bot.bar {
  height: 60vh;
}
.actionSide-bot.bar.moveit {
  transform: translate3d(0, -60vh, 0);
}
.actionSide-bot.moveit {
  transform: translate3d(0, -100vh, 0);
}
.actionSide-top {
  position: absolute;
  left: 0;
  top: -100vh;
  transition: all 0.2s ease;
}
.actionSide-top.moveit {

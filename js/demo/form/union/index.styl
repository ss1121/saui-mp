.boxer
  width 100vw
  height: 100vh
  overflow-x hidden
  position relative
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding 15rpx
  .ul
    // display: flex
    // flex-direction: column
    padding 0 10px
  .item-normal
    width 100%
    font-size 14px
    padding 10px 0
    border-bottom 1px solid #d3d3d3
    &:last-child
      border-bottom none
    .htitles
      display-flex(row nowrap, space-between, center, center)
      width 100%
      .item-icon
        font-size 14px
        &:after
          content: '\e618'